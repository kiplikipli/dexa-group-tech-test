{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "entryFile": "apps/gateway/src/main",
  "sourceRoot": "./",
  "projects": {
    "app-config": {
      "type": "library",
      "root": "libs/app-config",
      "entryFile": "index",
      "sourceRoot": "libs/app-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/app-config/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "apps/auth/src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "dayjs": {
      "type": "library",
      "root": "libs/dayjs",
      "entryFile": "index",
      "sourceRoot": "libs/dayjs/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dayjs/tsconfig.lib.json"
      }
    },
    "employee": {
      "type": "application",
      "root": "apps/employee",
      "entryFile": "apps/employee/src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/employee/tsconfig.app.json"
      }
    },
    "firebase": {
      "type": "library",
      "root": "libs/firebase",
      "entryFile": "index",
      "sourceRoot": "libs/firebase/src",
      "compilerOptions": {
        "tsConfigPath": "libs/firebase/tsconfig.lib.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "apps/gateway/src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "history": {
      "type": "application",
      "root": "apps/history",
      "entryFile": "main",
      "sourceRoot": "apps/history/src",
      "compilerOptions": {
        "tsConfigPath": "apps/history/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "application",
      "root": "apps/notification",
      "entryFile": "main",
      "sourceRoot": "apps/notification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification/tsconfig.app.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}